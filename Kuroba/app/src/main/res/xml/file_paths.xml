<?xml version="1.0" encoding="utf-8"?>
<paths>
    <!-- This one is so that we are able to share stuff from the file cache into other apps/install apks -->
    <files-path
        name="dc"
        path="disk_cache/" />
    <external-files-path
        name="exdc"
        path="disk_cache/" />

    <!-- This one is for file sharing into other apps -->
    <cache-path
        name="sf"
        path="share_files/" />

    <!-- This one is for temporary files -->
    <files-path
        name="tf"
        path="temp_files/" />
</paths>
